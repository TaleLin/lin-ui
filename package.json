{
  "name": "lin-ui",
  "version": "0.8.1",
  "description": "A high quality UI components library with MiniProgram",
  "main": "app.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "gulp -f build/index.js dev",
    "build": "gulp -f build/index.js build",
    "watch": "gulp -f build/index.js watch",
    "lint": "eslint ./src/**/*.js",
    "test": "jest",
    "release": "sh build/release.sh",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -n .convention-changelog-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaleLin/lin-ui.git"
  },
  "author": "Lin Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TaleLin/lin-ui/issues"
  },
  "miniprogram": "dist",
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "homepage": "http://doc.mini.7yue.pro",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "child_process": "^1.0.2",
    "del": "^5.0.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^3.9.3",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-jsonminify": "^1.1.0",
    "gulp-less": "^4.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify-es": "^2.0.0",
    "gulp-util": "^3.0.8",
    "husky": "^1.0.0-rc.6",
    "jest": "^24.8.0",
    "less": "^2.7.3",
    "lodash": "^4.17.15",
    "miniprogram-simulate": "^1.0.7"
  },
  "dependencies": {}
}
