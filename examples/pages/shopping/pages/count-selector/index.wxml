<view class="l-class container-count">
  <view class="l-symbol-class symbol {{count<=min|| disabled?'disabled':'abled'}}" 
        catchtap="{{count<=min|| disabled?'doNothing':'reduceTap'}}">
    <text>-</text>
  </view>
  <view class="l-count-class count">{{count}}</view>
  <view class="l-symbol-class symbol {{count>=max|| disabled?'disabled':'abled'}}" 
        catchtap="{{count>=max|| disabled?'doNothing':'addTap'}}">
       <text>+</text>
  </view>
</view>